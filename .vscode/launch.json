{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "join checkpoints",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/fit/checkpoints/join_checkpoints_autodecoders.py",
            // "args": [
            // ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "NETWORKX_AUTOMATIC_BACKENDS": "networkx",
                "JAX_DISABLE_JIT": "False",
            },
        },
        {
            "name": "refine autodecoder coil",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/charts/refine_chart.py",
            "args": [
                "--fit=charts/config/refine_charts_coil.py",
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "NETWORKX_AUTOMATIC_BACKENDS": "networkx",
                "JAX_DISABLE_JIT": "False",
            },
        },
        {
            "name": "train coil",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pinns/diffusion_single_gpu_autodecoder/main.py",
            "args": [
                "--config=pinns/diffusion_single_gpu_autodecoder/configs/coil.py",
                "--config.autoencoder_checkpoint.step=60000",
                "--config.mode=train"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "JAX_DISABLE_JIT": "False",
            },
        },
        {
            "name": "eval coil",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pinns/diffusion_single_gpu_autodecoder/main.py",
            "args": [
                "--config=pinns/diffusion_single_gpu/configs/coil.py",
                "--config.autoencoder_checkpoint.step=60000",
                "--config.eval.step=9999",
                "--config.mode=eval"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "JAX_DISABLE_JIT": "False",
            },
        }
    ]
}